class Solution {
    public int searchInsert(int[] nums, int target) {
        int ind=-1;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==target){
                return i;
            }
            else{
                if(nums[i]<target){
                    ind=i;
                }
               
            }
        }
        return ind+1;
    }
}
