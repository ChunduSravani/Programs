public class MissingNumberUsingArray {
	public static void main(String[] args) {
		int arr[]= {1,2,4,3,5,0,7,8,9};
		int hash[]=new int[arr.length+1];
		for(int i=0;i<arr.length;i++) {
			hash[arr[i]]=1;
		}
		for(int i=0;i<hash.length;i++) {
			if(hash[i]==0) {
				System.out.println(i);
			}
		}
		System.out.println();

	}

}
